type Time = int;
type Message = 
 record {
   author: text;
   content: text;
   time: Time;
 };
service : {
  follow: (principal) -> ();
  followPost: (principal) -> (vec Message);
  follows: () -> (vec principal) query;
  get_name: () -> (text);
  post: (text, text) -> ();
  posts: () -> (vec Message) query;
  postsByTime: (Time) -> (vec Message) query;
  set_name: (text) -> ();
  timeline: (Time) -> (vec Message);
}
